<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <gradient
                android:startColor="#fffa4007"
                android:endColor="@color/dataOut"
                android:angle="0"/>
            <stroke
                android:width="0dp"
                android:color="#01264e80"/>
            <corners android:radius="2dp"/>
            <padding
                android:left="1dp"
                android:top="1dp"
                android:right="1dp"
                android:bottom="1dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle"/>
    </item>
    <item android:top="0dp">
        <shape>
            <gradient
                android:startColor="@color/colorBG"
                android:endColor="@color/colorBG"
                android:angle="0"/>
            <corners android:radius="4dp"/>
        </shape>
    </item>
</layer-list>
