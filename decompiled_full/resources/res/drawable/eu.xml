<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <gradient
                android:startColor="#ff302b2b"
                android:endColor="#fffa4007"
                android:angle="360"/>
            <stroke
                android:width="0dp"
                android:color="#01264e80"/>
            <corners android:radius="12dp"/>
            <padding
                android:left="2dp"
                android:top="2dp"
                android:right="2dp"
                android:bottom="2dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle"/>
    </item>
</layer-list>
